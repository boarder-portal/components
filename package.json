{
  "name": "boarder-components",
  "version": "1.3.2",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rm -rf ./dist && rollup -c && tsc --project ./components.tsconfig.json",
    "gallery:start": "npm-run-all --parallel gallery:client:build:watch gallery:server:build:watch gallery:server:start:watch css:watch:gallery css:watch:src",
    "gallery:client:build:watch": "webpack -w -c ./gallery/webpack/client.config.js",
    "gallery:server:build:watch": "webpack -w -c ./gallery/webpack/server.config.js",
    "gallery:server:start:watch": "supervisor ./gallery/build/server/main.js",
    "css:watch:gallery": "tcm -p gallery/**/*.pcss --watch",
    "css:watch:src": "tcm -p src/**/*.pcss --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boarder-portal/components.git"
  },
  "author": "atooxa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boarder-portal/components/issues"
  },
  "homepage": "https://github.com/boarder-portal/components#readme",
  "peerDependencies": {
    "react": "^17.0.2",
    "classnames": "^2.3.1"
  },
